{
  "name": "@tsctx/test-repo",
  "version": "0.0.1",
  "type": "module",
  "author": "tsctx",
  "license": "MIT",
  "sideEffects": false,
  "main": "./index.js",
  "types": "./types/index.d.ts",
  "readme": "README.md",
  "exports": {
    ".": {
      "default": "./index.js"
    }
  },
  "typesVersions": {
    "*": {}
  },
  "scripts": {
    "build": "yarn build:types",
    "prebuild:types": "node ./scripts/pretypes.mjs",
    "build:types": "tsc -p ./tsconfig.types.json",
    "lint": "biome lint ./",
    "lint:ci": "biome ci --formatter-enabled=true --linter-enabled=true ./",
    "lint:tsc": "tsc --noEmit",
    "lint:fix": "biome lint --write --unsafe ./",
    "format": "biome format ./",
    "format:fix": "biome format --write ./",
    "update:wpt": "git-node wpt fetch"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.14.11",
    "mitata": "^0.1.11",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "undici": "^6.19.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsctx/web-apis.git"
  },
  "bugs": {
    "url": "https://github.com/tsctx/web-apis/issues"
  }
}
